{
  "name": "@mojolicious/server-starter",
  "version": "2.1.2",
  "description": "UNIX superdaemon with support for socket activation",
  "keywords": [
    "superdaemon",
    "tcp"
  ],
  "author": "Sebastian Riedel <kraih@mojolicious.org>",
  "license": "Artistic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojolicious/server-starter.git"
  },
  "bugs": {
    "url": "https://github.com/mojolicious/server-starter/issues"
  },
  "homepage": "https://mojolicious.org",
  "scripts": {
    "coverage": "c8 tap --disable-coverage --allow-empty-coverage test/*.js",
