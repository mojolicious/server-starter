    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "tap": "^21.0.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "os": [
    "!win32"
